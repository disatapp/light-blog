{
    "name": "disatapp/light-blog",
    "description": "A light cms built with laravel and uses vuejs on the frontend",
    "type": "project",
    "require": {
        "davejamesmiller/laravel-breadcrumbs": "5.x"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pavin Disatapundhu",
            "email": "disatapp@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Disatapp\\LightBlog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Disatapp\\LightBlog\\LightBlogProvider"
            ],
            "aliases": {
                "LightBlog": "Disatapp\\LightBlog\\Facades\\LightBlog"
            }
        }
    }

}
